{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "wrangler pages dev dist --d1=ai-presenter-db --local",
    "deploy": "wrangler pages deploy dist --project-name ai-presenter",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply ai-presenter-db --local",
    "db:migrate:prod": "wrangler d1 migrations apply ai-presenter-db"
  },
  "dependencies": {
    "hono": "^4.10.7"
  },
  "devDependencies": {
    "@hono/vite-cloudflare-pages": "^0.4.3",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  }
}
